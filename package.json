{
  "name": "sunny-quotes",
  "version": "2.0.0",
  "description": "It's Always Sunny in Philadelphia Quote Generator",
  "main": "index.js",
  "scripts": {
    "watch": "npm run clean; npm run build; npm start",
    "start": "nodemon -e js,handlebars --watch server -r dotenv/config server/index.js",
    "clean": "rm -rf ./public/js",
    "build": "NODE_ENV=development webpack --config ./config/webpack/webpack.config.js -d --progress --profile",
    "test": "mocha --compilers babel-core/register ./test/*test.js"
  },
  "keywords": [],
  "author": "RDP",
  "license": "ISC",
  "dependencies": {
    "express": "4.16.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "webpack-dev-middleware": "2.0.4"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "chai": "4.1.2",
    "console-polyfill": "0.3.0",
    "css-loader": "0.28.9",
    "dotenv": "5.0.0",
    "enzyme": "3.3.0",
    "express-handlebars": "3.0.0",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "less-plugin-clean-css": "1.5.1",
    "mocha": "5.0.0",
    "nodemon": "npm 1.14.12",
    "postcss-loader": "^2.1.1",
    "style-loader": "0.20.1",
    "webpack": "3.10.0"
  }
}
