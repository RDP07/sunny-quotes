{
  "name": "sunny-quotes",
  "version": "2.0.2",
  "description": "It's Always Sunny in Philadelphia Quote Generator",
  "main": "index.js",
  "engines": {
    "node": "10.9.0"
  },
  "scripts": {
    "watch": "npm run clean; npm run build; npm start",
    "start": "nodemon -e js,handlebars --watch server -r dotenv/config server/index.js",
    "clean": "rm -rf ./public/js",
    "build": "NODE_ENV=development webpack --config ./config/webpack/webpack.config.js -d --progress --profile",
    "build:prod": "NODE_ENV=production webpack --config ./config/webpack/webpack.config.js -d --progress --profile",
    "test": "mocha --compilers babel-core/register ./test/*test.js",
    "postinstall": "npm run build"
  },
  "keywords": [],
  "author": "RDP",
  "license": "ISC",
  "dependencies": {
    "express": "4.16.2",
    "fill-range": "5.0.0",
    "react": "16.2.0",
    "react-dom": "16.4.2",
    "webpack-dev-middleware": "2.0.4"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.24.1",
    "chai": "4.1.2",
    "console-polyfill": "0.3.0",
    "css-loader": "0.28.9",
    "dotenv": "5.0.0",
    "enzyme": "^3.9.0",
    "express-handlebars": "3.0.0",
    "less": "3.0.4",
    "less-loader": "4.0.5",
    "less-plugin-clean-css": "1.5.1",
    "mocha": "5.0.0",
    "nodemon": "^1.18.10",
    "postcss-loader": "2.1.1",
    "prop-types": "15.6.1",
    "style-loader": "0.20.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  }
}
